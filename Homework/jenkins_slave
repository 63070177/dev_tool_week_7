pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: '', description: 'Enter your name')
        string(name: 'Ages', defualtValue: '', description: 'Enter your age')
    }
    stages {
        stage('Test Python') {
            steps {
                sh 'python myscript.py ${params.NAME} --age ${params.Ages}'
            }
        }
    }
}